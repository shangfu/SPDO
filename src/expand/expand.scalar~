 def expandoracle(code: Long, dep: Int): List[Long] = {
 	val a = new ListBuffer[Long]()
 	var i = 0
	for (i<-5 to (dep-1)) {
	 	a+= (code >> ((dep-i)*4) << 4) + i
	 }
	return a.toList
 }
  
val oracle = sc.textFile("oracles.out").flatMap(line => expandoracle( line.split(" ")(0).toLong, line.split(" ")(2).toByte) ).distinct()

oracle.saveAsSequenceFile("oracles/")
